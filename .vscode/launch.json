{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug All Tests",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/output/build/${config:neoactivemq.buildPreset}/bin/neoactivemq-test.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "console": "integratedTerminal"
        },
        {
            "name": "Debug Failover Tests",
            "type": "cppvsdbg",
