        {
            "name": "Debug Tests (Select Preset)",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/output/build/${input:buildPreset}/bin/neoactivemq-test.exe",
            "args": [
                "-test",
                "${input:testName}"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "console": "integratedTerminal"
        },
        {
            "name": "Debug Example (Select Preset)",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/output/build/${input:buildPresetNonTest}/bin/examples/example_bo-store-test_BoStoreSubscriberMain.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "PATH",
                    "value": "${workspaceFolder}/output/build/${input:buildPresetNonTest}/bin;${env:PATH}"
                }
            ],
            "console": "integratedTerminal"
        }
    ],
    "inputs": [
        {
            "id": "testName",
            "type": "promptString",
            "description": "Enter the test name (e.g., activemq::transport::failover::FailoverTransportTest::testMethod)",
            "default": ""
        },
        {
            "id": "buildPreset",
            "type": "pickString",
            "description": "Select build preset",
            "options": [
                "x86-windows-debug-test",
                "x86-windows-release-test",
                "x64-windows-debug-test",
                "x64-windows-release-test",
                "x64-windows-debug-no-ssl-test",
                "x64-windows-release-no-ssl-test"
            ],
            "default": "${config:neoactivemq.buildPreset}"
        },
        {
            "id": "buildPresetNonTest",
            "type": "pickString",
            "description": "Select build preset (non-test)",
            "options": [
                "x86-windows-debug",
                "x86-windows-release",
                "x64-windows-debug",
                "x64-windows-release",
                "x64-windows-debug-no-ssl",
                "x64-windows-release-no-ssl"
            ],
            "default": "${config:neoactivemq.buildPresetNonTest}"
        },
        {
            "id": "corruptedTestName",
            "type": "pickString",
            "description": "Select a Corrupted Message test to run",
            "options": [
                "activemq::transport::CorruptedMessageTest::testCorruptedFirstMessage",
                "activemq::transport::CorruptedMessageTest::testCorruptedMessageBetweenValidMessages",
                "activemq::transport::CorruptedMessageTest::testContinueAfterCorruptedMessage",
                "activemq::transport::CorruptedMessageTest::testMultipleCorruptedMessages",
                "activemq::transport::CorruptedMessageTest::testCorruptedMessageWithoutMessageId",
                "activemq::transport::CorruptedMessageTest::testMaxConsecutiveErrors",
                "activemq::transport::CorruptedMessageTest::testPoisonAckSent",
                "activemq::transport::CorruptedMessageTest::testStreamResyncAfterSingleCorruption",
                "activemq::transport::CorruptedMessageTest::testCorruptedMessageDuringFailover",
                "activemq::transport::CorruptedMessageTest::testCorruptionInDifferentMessageParts",
                "activemq::transport::CorruptedMessageTest::testCorruptedMessageNonDurableConsumer",
                "activemq::transport::CorruptedMessageTest::testCorruptedMessageDurableTopicSubscriber",
                "activemq::transport::CorruptedMessageTest::testPoisonAckForDurableSubscriber",
                "activemq::transport::CorruptedMessageTest::testEOFDuringMessageIdRead",
                "activemq::transport::CorruptedMessageTest::testEOFDuringPropertiesRead",
                "activemq::transport::CorruptedMessageTest::testEOFDuringBodyRead"
            ],
            "default": "activemq::transport::CorruptedMessageTest::testEOFDuringPropertiesRead"
        }
    ]
}
